{
  "expo": {
    "name": "Taguig NutriApp",
    "slug": "Taguig-NutriApp",
    "version": "1.0.02",
    "orientation": "portrait",
    "icon": "./assets/images/withbgthinkbig-01.png",
    "scheme": "taguig-nutriapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/FixedSquareNoBg.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.joelgamba.TaguigNutriApp"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/FixedSquareNoBg.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/withbgthinkbig-01.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "005f41e5-06ed-4646-addf-f068457abd0a"
      }
    },
    "owner": "joelgamba"
  }
}
